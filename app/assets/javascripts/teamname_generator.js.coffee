  #
  # Returns a random element from the array 'arr'
  #
  randomElement: (arr) ->
    return arr[Math.floor(Math.random() * arr.length)]

  #
  # Returns true if the string 's' contains one of the
  # template strings.
  #
  hasTemplate: (s) ->
    for template in teamnameGenerator.categories
      return true if s.indexOf(template) >= 0

  #
  # Generates new team name.
  #
  spawn: (locale) ->
    # Currently only en and fr locale is supported
    if locale == "en"
      name = teamnameGenerator.randomElement(teamnameGenerator.adjectives) +
        teamnameGenerator.randomElement(teamnameGenerator.pluralNouns) +
        teamnameGenerator.randomElement(teamnameGenerator.verbs) +
        teamnameGenerator.randomElement(teamnameGenerator.adverbs)
    else if locale == "fr"
      name = teamnameGenerator.randomElement(teamnameGenerator.pluralNounsFr) +
        teamnameGenerator.randomElement(teamnameGenerator.adjectivesFr) +
        teamnameGenerator.randomElement(teamnameGenerator.verbsFr) +
        teamnameGenerator.randomElement(teamnameGenerator.adverbsFr)
    else
      name = "Current language not supported"

    return name

window.teamnameGenerator = teamnameGenerator
window.suggestTeamname = ->
  teamnameGenerator.spawn(frontendLocale())

